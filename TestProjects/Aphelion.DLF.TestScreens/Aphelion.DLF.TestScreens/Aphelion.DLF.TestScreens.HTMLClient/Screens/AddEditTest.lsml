    <Screen.Properties>
      <ScreenProperty
        Name="Test"
        PropertyType="Test">
        <ScreenProperty.Attributes>
          <IsParameter />
        </ScreenProperty.Attributes>
      </ScreenProperty>
      <ScreenCollectionProperty
        Name="TestSteps"
        PageSize="15"
        PropertyType="CollectionView$TestStep">
        <ScreenCollectionProperty.Query>
          <ScreenQueryDefinition
            Name="TestStepsQuery"
            ReturnType="TestStep*">
            <ScreenQueryDefinition.FilterExpression>
              <QueryFilterExpressionTree />
            </ScreenQueryDefinition.FilterExpression>
            <ScreenQueryDefinition.Source>
              <QuerySourceExpressionTree>
                <ChainExpression>
                  <MemberExpression
                    Member="AddEditTest/Properties[Test]" />
                  <MemberExpression
                    Member="TestSteps" />
                </ChainExpression>
              </QuerySourceExpressionTree>
            </ScreenQueryDefinition.Source>
          </ScreenQueryDefinition>
        </ScreenCollectionProperty.Query>
        <ScreenExpressionTree>
          <ChainExpression>
            <CallExpression
              Target="AddEditTest/Properties[TestSteps]/Query" />
          </ChainExpression>
        </ScreenExpressionTree>
      </ScreenCollectionProperty>
    </Screen.Properties>
  </Screen>
  <Application.Methods>
    <ApplicationMethod
      Name="showAddEditTest">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="AddEditTest" />
      </ApplicationMethod.Attributes>
      <ApplicationMethodParameter
        Name="Test"
        ParameterType="Test" />
    </ApplicationMethod>
  </Application.Methods>
</Application>